MAKEDEPEND		= gcc -MM
MAKEDEPENDOPTS	= 
SWIG 			= swig
SEDMI 			= sed -i
PYTHON 			= python
PYVER 			= 2.6
DOXYGEN 		= doxygen
USE_DOXYGEN 	= yes
SWIGFLAGS		= -w473 -w454 -fvirtual
EXT_LIB			= so
EXT_INP_SWIG	= i
EXT_OBJ_SWIG	= _wrap.cxx.o
EXT_SRC_HEADER	= h
EXT_SRC_C 		= c
EXT_SRC_CPP 	= cpp
EXT_SRC_SWIG	= _wrap.cxx
KERNELNAME  	= Linux
MACHINE  		= i686
