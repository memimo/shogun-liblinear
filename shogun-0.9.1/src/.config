SRCDIR			= .
PARTS			=  libshogun libshogunui python python_modular
LIBRARY_PATH= LD_LIBRARY_PATH
LIBSHOGUN_MODE= internal
LIBSHOGUN_TARGET= libshogun.so.6.0
LIBSHOGUNUI_TARGET= libshogunui.so.3.1
CMDLINE_TARGET	= 
R_TARGET		= 
PYTHON_TARGET	= sg.so
OCTAVE_TARGET	= 
MATLAB_TARGET	= 
ELWMS_TARGET	= 
